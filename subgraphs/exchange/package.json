    "deploy-local": "graph deploy pancakeswap/exchange-v2 --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "remove-local": "graph remove pancakeswap/exchange-v2 --node http://127.0.0.1:8020",
    "remove-tmp1-local": "graph remove --node http://128.0.0.1:8020 QmPbWqCCnUxcDhe6ANcMGbn1QjVKHrEdbaKVDBV3c3btNc",
    "remove-tmp2-local": "graph remove --node http://128.0.0.1:8020 QmfGuiGdHVCbSf8TsVBcjTSn93udHVuwy1EFGyLKEfJ6ty"
  }
